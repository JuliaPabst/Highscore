{
  "openapi": "3.0.0",
  "info": {
    "title": "Highscore Application API",
    "version": "1.0.0"
  },
  "paths": {
    "/users": {
      "post": {
        "summary": "Create a new user",
        "responses": {
          "200": {
            "description": "User created successfully"
          }
        }
      }
    },
    "/login": {
      "post": {
        "summary": "User login",
        "responses": {
          "200": {
            "description": "User logged in successfully"
          }
        }
      }
    },
    "/scores": {
      "get": {
        "summary": "Get high scores",
        "responses": {
          "200": {
            "description": "High scores retrieved successfully"
          }
        }
      }
    }
  }
}
